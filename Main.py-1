3ï¸âƒ£ Main React App â€” src/App.jsx

import { useState } from "react";
import constellation from "./data/constellation.json";
import "./index.css";

const allSubsystems = [
  "AS1","AS2","AS3","AS4","AS5","AS6","AS7","AS8","AS9","AS10","AS11","AS12"
];

export default function App() {
  const [filter, setFilter] = useState("All");

  const filteredRepos =
    filter === "All"
      ? constellation.repos
      : constellation.repos.filter(r => r.subsystems.includes(filter));

  return (
    <div className="p-6">
      <h1 className="text-center text-4xl font-bold mb-10">
        ğŸŒŠâš›ï¸ğŸ’« AQARIONZ Constellation Dashboard
      </h1>

      <div className="mb-6 flex flex-wrap justify-center gap-2">
        <button
          className={`px-3 py-1 rounded-full ${filter==="All" ? "bg-cyan-500" : "bg-gray-800"}`}
          onClick={()=>setFilter("All")}
        >All</button>
        {allSubsystems.map(as=>(
          <button
            key={as}
            className={`px-3 py-1 rounded-full ${filter===as ? "bg-cyan-500" : "bg-gray-800"}`}
            onClick={()=>setFilter(as)}
          >{as}</button>
        ))}
      </div>

      <div className="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4">
        {filteredRepos.map(repo=>(
          <div key={repo.name} className="bg-gray-800 p-4 rounded-xl shadow hover:scale-105 transition transform">
            <h3 className="text-xl font-semibold">{repo.name}</h3>
            <p className="text-gray-300 mt-1">{repo.description}</p>
            <div className="mt-2 flex flex-wrap gap-1">
              {repo.subsystems.map(as=>(
                <span key={as} className="bg-cyan-700 px-2 py-1 rounded-full text-sm">{as}</span>
              ))}
            </div>
            <a href={repo.url} target="_blank" className="mt-3 inline-block text-cyan-400 font-medium hover:underline">
              View Repo
            </a>
          </div>
        ))}
      </div>
    </div>
  );
}


