    { name:"AtreyueTechnology", status:44, url:"https://github.com/starwell19/AtreyueTechnology" },
    { name:"AQARIONZ-TRONSIMZ", status:39, url:"https://github.com/aqarion/AQARIONZ-TRONSIMZ" }
];

const grid = document.getElementById("repo-grid");

repos.forEach(r=>{
    const c=document.createElement("div");
    c.className="repo-card";
    c.innerHTML = `
        <h3>${r.name}</h3>
        <p>${r.status}% complete</p>
        <a href="${r.url}" target="_blank">View Repo</a>
    `;
    grid.appendChild(c);
});


